{
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaFeatures": {
      "experimentalDecorators": 0
    }
  },
  "extends": "airbnb",
  "rules": {
    "max-len": ["error", 80],
    "import/extensions": 0,
    "linebreak-style": 0,
    "consistent-return": 0,
    "no-use-before-define": 0
  }
}